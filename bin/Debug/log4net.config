<log4net>
    <appender name="conLogger" type="log4net.Appender.ConsoleAppender">
        <layout type="log4net.Layout.PatternLayout">
            <conversionPattern value="%date [%-5level] [%thread] [%logger] %message%newline" />
        </layout>
    </appender>
    <root>
        <level value="INFO" />
        <appender-ref ref="conLogger" />
    </root>
</log4net>
